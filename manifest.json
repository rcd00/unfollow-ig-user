{
    "manifest_version": 2,
    "name": "Unfollow IG user",
    "version": "1.0",
    "permissions": ["tabs"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }
